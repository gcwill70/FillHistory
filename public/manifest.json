{
  "name": "FillHistory",
  "version": "1.5.0",
  "description": "Quickly fill in links from browser history.",
  "manifest_version": 3,
  "permissions": ["history", "activeTab", "contextMenus", "storage", "tabs"],
  "icons": {
    "256": "icon256.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "search": {
      "description": "Search for a link",
      "suggested_key": {
        "default": "Ctrl+Shift+L",
        "mac": "Command+Shift+L"
      }
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
